{
  "name": "recipelab",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "graphql:schema": "npx apollo schema:download --endpoint=http://localhost:5000/graphql",
    "graphql:codegen": "npx apollo client:codegen src/graphql/__generated__ --includes=src/**/*.{ts,tsx} --target typescript --outputFlat"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@os-team/i18next-react-native-language-detector": "^1.0.12",
    "@react-native-async-storage/async-storage": "^1.16.1",
    "@react-native-cookies/cookies": "^6.0.11",
    "@react-native-google-signin/google-signin": "^7.1.0",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/drawer": "^6.3.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.1.1",
    "apollo-client": "^2.6.10",
    "axios": "^0.26.0",
    "i18next": "^21.6.14",
    "react": "17.0.2",
    "react-i18next": "^11.16.1",
    "react-native": "0.67.1",
    "react-native-dropdown-picker": "^5.3.0",
    "react-native-gesture-handler": "^2.3.1",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^4.11.2",
    "react-native-reanimated": "^2.4.1",
    "react-native-safe-area-context": "^4.1.2",
    "react-native-screens": "^3.13.1",
    "react-native-searchable-dropdown": "^1.1.3",
    "react-native-select-dropdown": "^1.13.0",
    "react-native-vector-icons": "^9.1.0",
    "ts-jest": "^27.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-native": "^9.1.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.15",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "apollo": "^2.33.9",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17",
    "graphql": "^15.5.3",
    "**/graphql": "^15.5.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "allowJs": true
        }
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-native-community|@react-navigation|@react-native-modal)"
    ]
  }
}
